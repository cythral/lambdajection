{
    "$schema": "http://json.schemastore.org/template",
    "identity": "Lambdajection.Templates.Lambda",
    "author": "Cythral LLC",
    "classifications": [
        "Cloud",
        "Lambda"
    ],
    "name": "AWS Lambda using Lambdajection",
    "shortName": "lambdajection",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "namespace": {
            "type": "parameter",
            "description": "Namespace for the Lambda project",
            "replaces": "Project"
        },
        "version": {
            "type": "parameter",
            "description": "Version of Lambdajection to use",
            "replaces": "$(LambdajectionVersion)",
            "defaultValue": "__LambdajectionVersion__"
        },
        "encryption": {
            "type": "parameter",
            "description": "Whether to include the Encryption package",
            "datatype": "bool",
            "defaultValue": "false"
        }
    }
}
